<div class="jumbotron">
  <h1>Souelni</h1>
</div>
<table class=table>
  <thead>
    <tr>
      <th>Categorie</th>    
      <th>Titre</th>
      <th>Question</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questions.each do |question| %>
      <tr>
        <td><%= question.categorie %></td>
        <td><%= question.titre %></td>
        <td><%= question.question %></td>
        <td><%= question.created_at.to_date %></td>
        <td><%= link_to 'Show', question, class: 'btn btn-info' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


